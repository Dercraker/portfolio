// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id      String  @id @default(nanoid(11))
  message String
  username   String

  createdAt DateTime @default(now())
}

model Project {
  id    String  @id @default(nanoid(11))
  title String
  desc  String
  size  String
  link  String?
  date  DateTime?
  }